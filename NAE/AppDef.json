{
    "name": "Distributed Tensorflow for IBM POWER",
    "description": "Launch a Distributed Tensorflow GPU Cluster than runs between-graph replicated training and evaluation.",
    "author": "sfox",
    "licensed": true,
    "classifications": [
        "Machine Learning/Deep Learning",
        "IBM POWER/Machine Learning",
        "IBM POWER/Deep Learning"
    ],
    "machines": [
        "np8g4"
    ],
    "vault-types": [
        "BLOCK",
        "FILE"
    ],
    "commands": {
        "developer-server": {
            "path": "/sbin/init",
            "interactive": true,
            "name": "Developer Server",
            "description": "Launch a server session with SSH to test Tensorflow-capable python code.",
            "parameters": {}
        },
        "tensorboard-only": {
            "path": "/bin/bash",
            "interactive": true,
            "name": "tensorboard",
            "description": "Launch tensorboard",
            "machines": [
                "np8c0"
            ],
            "parameters": {
                "-l": {
                    "name": "-l",
                    "description": "-l",
                    "type": "CONST",
                    "value": "-l",
                    "positional": true,
                    "required": true
                },
                "-c": {
                    "name": "-c",
                    "description": "-c",
                    "type": "CONST",
                    "value": "-c",
                    "positional": true,
                    "required": true
                },
                "command": {
                    "name": "Command",
                    "description": "Command to run",
                    "type": "CONST",
                    "value": "tensorboard",
                    "positional": true,
                    "required": true
                },
                "logdiropt": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "--logdir",
                    "positional": true,
                    "required": true
                },
                "logdir": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "/data/tensorflow-output",
                    "positional": true,
                    "required": true
                }
            }
        }
    }
}
