{
    "name": "Tensorflow and Jupyter for TFaaS",
    "description": "Launch an instance with python libs, TensorFlow and Jupyter, ssh-enabled, CPU only",
    "author": "hanna.varakhobava",
    "licensed": true,
    "classifications": [
        "Uncategorized"
    ],
    "machines": [
        "n0",
        "n1",
        "n2",
        "n3",
        "n32",
        "n4",
        "n5",
        "nc3",
        "nc5",
        "ng2",
        "ng4",
        "ng5",
        "ng7",
        "ngd2",
        "ngd4",
        "ngd5",
        "ngd7",
        "ngo8",
        "ngq7"
    ],
    "vault-types": [
        "BLOCK",
        "FILE",
        "BLOCK_ARRAY",
        "OBJECT"
    ],
    "commands": {
        "developer-server": {
            "path": "/sbin/init",
            "interactive": true,
            "name": "Developer Server",
            "description": "Launch a server session with SSH to test Tensorflow-capable python code.",
            "parameters": {}
        },
        "tensorboard-only": {
            "path": "/bin/bash",
            "interactive": true,
            "name": "tensorboard",
            "description": "Launch tensorboard",
            "machines": [
                "np8c0"
            ],
            "parameters": {
                "-l": {
                    "name": "-l",
                    "description": "-l",
                    "type": "CONST",
                    "value": "-l",
                    "positional": true,
                    "required": true
                },
                "-c": {
                    "name": "-c",
                    "description": "-c",
                    "type": "CONST",
                    "value": "-c",
                    "positional": true,
                    "required": true
                },
                "command": {
                    "name": "Command",
                    "description": "Command to run",
                    "type": "CONST",
                    "value": "tensorboard",
                    "positional": true,
                    "required": true
                },
                "logdiropt": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "--logdir",
                    "positional": true,
                    "required": true
                },
                "logdir": {
                    "name": "--logdir",
                    "description": "logdir",
                    "type": "CONST",
                    "value": "/data/tensorflow-output",
                    "positional": true,
                    "required": true
                }
            }
        }
    }
}
